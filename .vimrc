set guioptions-=T
set backspace=2
set backspace=indent,eol,start
colorscheme monokai
" This is for gvim with 2x scaled Plasma 5 on 4k
" guifont format is different for Windoze
set guifont=PragmataPro\ 9
set autoread
set tabstop=4
set shiftwidth=4
set number
syntax on
set incsearch
set hlsearch
set showmatch
set ruler

